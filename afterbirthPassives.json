		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/9/98/My_Shadow_Icon.png?version=485500b3f8ed3e292921493013a5ee49",
		"descriptionHTML": "Each time Isaac takes damage, inflicts <a href=\"/Effects#Fear\" title=\"Effects\">Fear</a> on all enemies in the room and spawns a black <a href=\"/Maggot#Charger\" title=\"Maggot\">Charger</a> familiar that will attack enemies. The Charger will be killed if it is damaged too much."
	},
	"Night Light": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Night_Light",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/3/3b/Night_Light_Icon.png?version=6b47a964bfa0d0c5324d38f6467b14b1",
		"descriptionHTML": "A cone of light shines in the direction Isaac is facing that <a href=\"/Effects#Status_Effects\" title=\"Effects\">slows</a> enemies and enemy shots."
	},
	"No. 2": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/No._2",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/a/a5/No._2_Icon.png?version=227530aab02d554ff5e2b874cc5ea420",
		"descriptionHTML": "Continuously firing tears for several seconds causes Isaac to drop a <a href=\"/Butt_Bombs\" title=\"Butt Bombs\">Butt Bomb</a>. Butt Bombs explode for 50 damage like normal bombs, but also deal 10 damage to all enemies in the room and daze them for a short period of time."
	},
	"Papa Fly": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Papa_Fly",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/9/95/Papa_Fly_Icon.png?version=63e0e3da426836248b73e39eaaafb1f2",
		"descriptionHTML": "A blue fly familiar follows Isaac's movements on a 3 second delay. Papa Fly will fire tears at nearby enemies."
	},
	"Pay To Play": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Pay_To_Play",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/d/df/Pay_To_Play_Icon.png?version=42e1d467c70018f89eb77c08d0112cf9",
		"descriptionHTML": "This item turns all doors that require a key into doors which need a coin to enter instead. This affects shops, item rooms, libraries and other key doors."
	},
	"PJs": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/PJs",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/9/97/PJs_Icon.png?version=ddb57e148f68bfb15d0c3e59c4aceaff",
		"descriptionHTML": "Adds 4 Soul Hearts and fully restores Isaac's red health."
	},
	"Pupula Duplex": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Pupula_Duplex",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/f/f2/Pupula_Duplex_Icon.png?version=e4960fb41956968b9231f2d79312e1d0",
		"descriptionHTML": "Transforms Isaac's tears into a wide arc shape which gives them a much larger hitbox. Gives Isaac spectral tears which allows them to travel through objects in the environment (i.e. rocks)."
	},
	"Purity": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Purity",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/0/0b/Purity_Icon.png?version=f8763b9301163a8db28b61737c8b69fb",
		"descriptionHTML": "This item will boost one of Isaac's stats depending on the color of the aura around him. The aura will disappear upon taking damage and will reappear with a random color upon entering a new room. A red aura boosts damage, a blue aura boosts rate of fire, a yellow aura boosts speed, and an orange aura boosts range."
	},
	"Obsessed Fan": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Obsessed_Fan",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/c/c5/Obsessed_Fan_Icon.png?version=808b9a73fc210401df3308f41bf4f86a",
		"descriptionHTML": "A purple fly familiar that follows Isaac's movements on a 3 second delay and deals contact damage to enemies."
	},
	"Restock": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Restock",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/a/a6/Restock_Icon.png?version=3512bc0e5cdcb753027c0bb0af7b2601",
		"descriptionHTML": "Causes <a href=\"/Shop\" title=\"Shop\">shops</a> to instantly restock their items when they are bought. Granted by default for every character in Greed Mode."
	},
	"Rune Bag": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Rune_Bag",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/a/a4/Rune_Bag_Icon.png?version=825b9b42699c4a6d469b044eb2000e71",
		"descriptionHTML": "A bag that follows Isaac and drops a random mystic rune every 3 rooms."
	},
	"Sack Head": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Sack_Head",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/4/48/Sack_Head_Icon.png?version=c7ca29d4d4395aa90181e448932570ac",
		"descriptionHTML": "Greatly increases the chance to find sacks."
	},
	"Scatter Bombs": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Scatter_Bombs",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/5/51/Scatter_Bombs_Icon.png?version=c411194267df282e245e362fe6276b0e",
		"descriptionHTML": "Adds 5 bombs. Isaac's bombs explode into 2-4 tiny bombs."
	},
	"Seraphim": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Seraphim",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/5/54/Seraphim_Icon.png?version=0115ea0ce9338d799c8d36509193d983",
		"descriptionHTML": "An angel familiar which follows Isaac and fires homing tears which deal 10 damage at -25% shot speed and a reduced fire rate."
	},
	"Serpent's Kiss": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Serpent%27s_Kiss",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/b/be/Serpent%27s_Kiss_Icon.png?version=8567fb91d65105dd6ccde320ddaae4e6",
		"descriptionHTML": "Tears now have a random chance to apply a poison effect, causing 4 or 6 damage per tick to enemies (Similar to The Common Cold item). Isaac now deals poison damage on contact with enemies similar to The Virus item (Damage over time 4 or 6 per tick). Enemies that were poisoned have a chance to drop a black sin heart when killed."
	},
	"Sticky Bombs": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Sticky_Bombs",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/0/00/Sticky_Bombs_Icon.png?version=ceff795c7fc3ece54d58da115d4e17b7",
		"descriptionHTML": "+5 bombs. Bombs stick to enemies and spawn 2-4 <a href=\"/Blue_Spider\" title=\"Blue Spider\" class=\"mw-redirect\">Blue Spiders</a> if they kill an enemy."
	},
	"Spear Of Destiny": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Spear_Of_Destiny",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/a/a8/Spear_Of_Destiny_Icon.png?version=8caa0611f558e812da3cc899864655d6",
		"descriptionHTML": "In addition to tears, Isaac now holds a spear in front of him which deals damage equal to 2 times his tear damage. The spear cannot be thrown, but deals damage while it is in contact with an enemy."
	},
	"Spider Mod": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Spider_Mod",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/6/6f/Spider_Mod_Icon.png?version=694e6c5d1e56b51accb5ca1103d487f1",
		"descriptionHTML": "Displays Isaac's tear damage and enemy health bars. Spawns a gray spider familiar that wanders around and inflicts a random status effect on any enemies it comes into contact with. Occasionally spawns a blue spider upon clearing a room."
	},
	"Succubus": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Succubus",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/c/ce/Succubus_Icon.png?version=9491a69890d284ec96a6a121f13e09f6",
		"descriptionHTML": "Spawns a flying familiar that bounces around the room with a black aura, dealing rapid damage to any enemies inside it. While standing in the aura, Isaac gains a 1.5x tear damage multiplier."
	},
	"Sworn Protector": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Sworn_Protector",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/0/03/Sworn_Protector_Icon.png?version=c72a598de2c769391d85c143797c5d0b",
		"descriptionHTML": "An orbital angel which does 7 contact damage per tick and blocks enemy shots. When it blocks a shot, it has a very small chance to drop an <a href=\"/Pickups#Eternal_Hearts\" title=\"Pickups\">eternal heart</a>."
	},
	"Tech X": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Tech_X",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/e/e6/Tech_X_Icon.png?version=b50c8d50dda1e7e40271051b175622f6",
		"descriptionHTML": "Tears are replaced with laser rings that pierce through enemies and deal damage to enemies within them. Can be charged to increase the size of the ring."
	},
	"The Wiz": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/The_Wiz",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/f/f4/The_Wiz_Icon.png?version=8f9814b27a7591b6c6db38ae65a95017",
		"descriptionHTML": "Tears simultaneously fire out of both eyes diagonally, similar to the effect of the <a href=\"/Pills\" title=\"Pills\">R U A Wizard?</a> pill. Isaac's tears also travel above all obstacles."
	},
	"Toxic Shock": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Toxic_Shock",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/a/ae/Toxic_Shock_Icon.png?version=e2e37e5fb77a7e0389526101596d4b96",
		"descriptionHTML": "Upon entering a room, poisons all enemies, dealing double Isaac's tear damage. Killing enemies leaves a small, short lasting pool of green <a href=\"/Creep\" title=\"Creep\">creep</a> on the ground."
	},
	"Tractor Beam": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Tractor_Beam",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/4/45/Tractor_Beam_Icon.png?version=0674598304dd253043b989ad35190843",
		"descriptionHTML": "Isaac emits a beam from his face. Isaac's tears travel in the path of the beam."
	},
	"Zodiac": {
		"wikiPage": "http://bindingofisaacrebirth.gamepedia.com/Zodiac",
		"thumbnail": "http://hydra-media.cursecdn.com/bindingofisaacrebirth.gamepedia.com/a/ab/Zodiac_Icon.png?version=6681844c398528e67c480e52a3582b92",
		"descriptionHTML": "Zodiac will give Isaac a random <a href=\"/Signs_Of_The_Zodiac\" title=\"Signs Of The Zodiac\">zodiac item</a> effect that changes after every floor."
	}
}